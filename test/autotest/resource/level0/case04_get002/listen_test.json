{
  "process": "listen_test",
  "systemability": [
    {
      "name": 1494,
      "libpath": "liblisten_test.z.so",
      "run-on-create": true,
      "distributed": false,
      "dump_level": 1,
      "start-on-demand": {
        "commonevent": [
          {
            "name": "usual.event.SCREEN_ON"
          }
        ]
      },
      "stop-on-demand": {
        "recycle-delaytime": 1000,
        "commonevent": [
          {
            "name": "usual.event.SCREEN_OFF"
          }
        ]
      }
    }
  ]
}