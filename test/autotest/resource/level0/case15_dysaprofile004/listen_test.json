{
  "process": "listen_test",
  "systemability": [
    {
      "name": 1494,
      "libpath": "liblisten_test.z.so",
      "run-on-create": false,
      "distributed": true,
      "dump_level": 1,
      "start-on-demand": {
        "allow-update": true,
        "deviceonline": [
          {
            "name": "deviceonline",
            "value": "on"
          }
        ],
        "commonevent": [
          {
            "name": "usual.event.wifi.POWER_STATE",
            "value": ""
          },
          {
            "name": "usual.event.SCREEN_ON",
            "value": ""
          }
        ]
      },
      "stop-on-demand": {
        "allow-update": true,
        "deviceonline": [
          {
            "name": "deviceonline",
            "value": "off"
          }
        ],
        "commonevent": [
          {
            "name": "usual.event.SCREEN_OFF",
            "value": ""
          }
        ]
      }
    }
  ]
}